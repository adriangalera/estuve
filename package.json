{
  "name": "estuve",
  "version": "1.0.0",
  "description": "estuve.eu lets you visualize all your gpx tracks in a leaflet map",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "test": "vitest --run && start-server-and-test dev http://localhost:5173 test:ui",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest --coverage",
    "test:ui": "cypress run",
    "test:ui:ci": "start-server-and-test dev http://localhost:5173 test:ui",
    "preview": "vite build && vite preview",
    "build": "vite build",
    "deploy": "vite build && touch dist/CNAME && echo 'estuve.eu' > dist/CNAME && gh-pages -d dist"
  },
  "keywords": [],
  "author": "Adrian Galera",
  "license": "ISC",
  "dependencies": {
    "geojson-vt": "^4.0.2",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "leaflet": "^1.9.4",
    "leaflet-easybutton": "^2.4.0",
    "leaflet-geojson-vt": "^1.1.0",
    "leaflet.markercluster": "^1.5.3",
    "leaflet.markercluster.layersupport": "^2.0.1",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@vitest/coverage-v8": "^3.0.8",
    "cypress": "^14.2.0",
    "gh-pages": "^6.3.0",
    "jsdom": "^26.0.0",
    "start-server-and-test": "^2.0.11",
    "vite": "^6.2.1",
    "vitest": "^3.0.8"
  }
}
